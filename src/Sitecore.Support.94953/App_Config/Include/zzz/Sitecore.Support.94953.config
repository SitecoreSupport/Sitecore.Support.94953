      <indexConfigurations>
        <defaultCloudIndexConfiguration>
          <documentBuilderType>Sitecore.Support.ContentSearch.Azure.CloudSearchDocumentBuilder, Sitecore.Support.94953.AzureSearch</documentBuilderType>
        </defaultCloudIndexConfiguration>
      </indexConfigurations>
    </contentSearch>
  </sitecore>
</configuration>